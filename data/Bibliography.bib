@Article{ISI:000279225000012,
author = {Savage, David W and Martell, David L and Wotton, B Mike}, 
title = {Evaluation of two risk mitigation strategies for dealing with fire-related uncertainty in timber supply modelling}, 
journal = {CANADIAN JOURNAL OF FOREST RESEARCH-REVUE CANADIENNE DE RECHERCHE FORESTIERE}, 
volume = {{40}}, 
number = {{6}}, 
pages = {{1136–1154}}, 
year = {2010}, 
abstract = {We embedded a linear programming timber harvest scheduling model into an aspatial stochastic simulation model of a flammable forest to evaluate two fire risk mitigation strategies. The harvest scheduling model is solved repeatedly to produce harvest schedules within a rolling planning horizon framework. The risk mitigation strategies we examined were (1) whether or not to account for fire in the planning model and (2) replanning interval. We evaluated those strategies under four representative fire regimes. We found that accounting for fire in the planning model reduced the harvest volume variability as fire activity increased (i. e., for average annual burn fractions >= 0.45%), but replanning intervals over a range of 1 to 10 years had little impact on harvest volume variability. We also developed a risk analysis decision-making aid that forest managers can use to help deal with fire-related uncertainty. Our results suggest that risk-averse forest managers should account for fire while planning, especially when burn fractions exceed 0.45%.}, 
location = {1200 MONTREAL ROAD, BUILDING M-55, OTTAWA, ON K1A 0R6, CANADA {Savage, DW (Reprint Author), Univ Toronto, Fac Forestry, 33 Willcocks St, Toronto, ON M5S 3B3, Canada. Savage, David W.; Martell, David L., Univ Toronto, Fac Forestry, Toronto, ON M5S 3B3, Canada. Wotton, B. Mike, Nat Resources Canada, Canadian Forest Serv, Great Lakes Forestry Ctr, Marie, ON P6A 2E5, Canada.}}, 
