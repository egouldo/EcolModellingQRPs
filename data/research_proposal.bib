title = {Phylotocol: Promoting transparency and overcoming bias in phylogenetics}, 
journal = {}, 
volume = {}, 
number = {}, 
pages = {}, 
year = {2018}, 
abstract = {Research products that lack transparency and are influenced by confirmation
